{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.{jpg,jpeg,png,gif,webp,svg}",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "**/*.{mp4,mp3,ogg,webm}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}